{
    "name": "hellofuzzy",
    "displayName": "helloFuzzy",
    "description": "A fuzzy file finder",
    "version": "0.0.1",
    "publisher": "henryiis",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:hellofuzzy.findFile"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "hellofuzzy.findFile",
                "title": "Go to File",
                "category": "helloFuzzy"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
